#
# {{ ansible_managed }}
#
[program:{{ analytics_processors_service_name }}]

environment=DJANGO_SETTINGS_MODULE=lms.envs.aws,SERVICE_VARIANT=lms
command={{ edxapp_venv_bin }}/python {{ edxapp_venv_dir }}/src/rg-instructor-analytics-log-collector/run_processors.py --sleep_time {{ analytics_processors_sleep_time }} --aliases {{ analytics_processors_aliases }} --delete-logs
user={{ common_web_user }}
stdout_logfile={{ supervisor_log_dir }}/%(program_name)s-stdout.log
stderr_logfile={{ supervisor_log_dir }}/%(program_name)s-stderr.log
killasgroup=true
stopasgroup=true
